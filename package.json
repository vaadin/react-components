{
  "name": "@vaadin/react-components",
  "version": "0.0.0",
  "description": "React wrappers for Vaadin components",
  "type": "module",
  "module": "dist/index.js",
  "main": "dist/index.js",
  "files": [
    "dist/**/*.js",
    "dist/**/*.js.map"
  ],
  "scripts": {
    "build:analyze": "node --loader ts-node/esm scripts/analyze.ts",
    "build:generate": "node --loader ts-node/esm scripts/generate.ts",
    "build:typescript": "tsc",
    "clean": "rimraf dist build",
    "build": "npm run build:analyze && npm run build:generate && npm run build:typescript",
    "serve": "node --loader ts-node/esm node_modules/.bin/web-dev-server -c web-dev-server.config.ts",
    "test": "node --loader ts-node/esm node_modules/.bin/web-test-runner --config web-test-runner.config.ts test/**/*.test.ts test/**/*.test.tsx"
  },
  "repository": {
    "type": "git",
    "url": "github.com/vaadin/react-components"
  },
  "dependencies": {
